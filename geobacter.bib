journal = {Nature Reviews Microbiology},
keywords = {bacteria,bacteria metabolism,bioelectric energy sources,carbon dioxide,carbon dioxide metabolism,electricity,electrochemistry,electrochemistry methods,electron transport,electron transport physiology,fermentation,microbiological techniques,microbiological techniques methods},
