issn = {1873-4863},
journal = {Journal of biotechnology},
keywords = {Computational Biology,Computational Biology: methods,Databases, Genetic,Genome, Fungal,Glycolysis,Saccharomycetales,Saccharomycetales: genetics,Software,User-Computer Interface,Yeasts,Yeasts: genetics},
month = aug,
number = {1},
pages = {118--26},
pmid = {21040748},
publisher = {Elsevier B.V.},
title = {{RAPYD--rapid annotation platform for yeast data.}},
